/**
 * @param {number} m
 * @param {number} n
 * @return {number}
 */
var uniquePaths = function(m, n) {
    function factorial(num){
        if(num === 0 || num === 1)
            return 1;
        return num * factorial(num-1);
    }
    return factorial(m+n-2) / (factorial(m-1) * factorial(n-1));
};
