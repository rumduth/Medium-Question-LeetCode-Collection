class Solution:
    def twoSum(self, numbers: List[int], target: int) -> List[int]:
        hash_map = {}
        for index, value in enumerate(numbers):
            if (target - value) in hash_map:
                return [hash_map[target-value] + 1,index + 1]
            hash_map[value] = index
        return [-1, -1]
